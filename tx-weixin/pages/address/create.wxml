<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','consignee','$event',[]],['addressData']]]]]}}" value="{{addressData.consignee}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['addressData']]]]]}}" value="{{addressData.phone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">城市</text><input class="input" placeholder="请选择城市" disabled="true" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" value="{{addressData.province+' '+addressData.city+' '+addressData.county}}" bindtap="__e"/><lotus-address vue-id="774e4354-1" lotusAddressData="{{lotusAddressData}}" data-event-opts="{{[['^choseVal',[['choseValue']]]]}}" bind:choseVal="__e" bind:__l="__l"></lotus-address><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">详细</text><input class="input" type="text" placeholder="街道、楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['addressData']]]]]}}" value="{{addressData.address}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaultAddress}}" color="#2AAC34" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>