.icon .mix-list-cell.b-b:after {
  left: 90rpx;
}
.mix-list-cell {
  display: flex;
  align-items: baseline;
  background: #FFFFFF;
  padding-left: 20rpx;
  padding: 20rpx 30rpx;
  line-height: 60rpx;
  position: relative;
}
.mix-list-cell.cell-hover {
  background: #fafafa;
}
.mix-list-cell.b-b:after {
  left: 30rpx;
}
.mix-list-cell .cell-icon {
  align-self: center;
  width: 56rpx;
  max-height: 60rpx;
  font-size: 38rpx;
}
.mix-list-cell .cell-more {
  align-self: center;
  font-size: 30rpx;
  color: #606266;
  margin-left: 10px;
}
.mix-list-cell .cell-tit {
  flex: 1;
  font-size: 28rpx;
  color: #303133;
  margin-right: 10rpx;
}
.mix-list-cell .cell-tip {
  border-top: #909399;
  font-size: 26rpx;
  color: #909399;
}
.btn::after {
  display: none;
}
.btn {
  width: 95%;
  position: absolute;
  opacity: 0;
  z-index: 99;
}

