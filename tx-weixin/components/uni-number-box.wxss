
.uni-numbox {
	/* position:absolute; */
	/* left: 30upx; */
	bottom: 0;
	display: flex;
	justify-content: flex-start;
	align-items: center;
	height: 46rpx;
	/* width:230upx; */
	/* height: 70upx; */
	/* background:#f5f5f5; */
}
.uni-numbox-minus,
.uni-numbox-plus {
	margin: 0;
	/* background-color: #f5f5f5; */
	width: 70rpx;
	/* height: 100%; */
	height: 46rpx;
	/* padding: 0 10rpx; */
	/* line-height: 70upx; */
	text-align: center;
	position: relative;
}
.uni-numbox-minus .yticon,
.uni-numbox-plus .yticon{
	font-size: 36rpx;
	color: #555;
}
.uni-numbox-minus {
	border-right: none;
	border-top-left-radius: 22rpx;
	border-bottom-left-radius: 22rpx;
	border: #EEEAEA solid 2rpx;
}
.uni-numbox-plus {
	border-left: none;
	border-top-right-radius: 22rpx;
	border-bottom-right-radius: 22rpx;
	border: #EEEAEA solid 2rpx;
}
.uni-numbox-value {
	position: relative;
	/* background-color: #f5f5f5; */
	width: 90rpx;
	color: #1B1C33;
	border-top: #EEEAEA solid 2rpx;
	border-bottom: #EEEAEA solid 2rpx;
	/* height: 50upx; */
	height: 46rpx;
	text-align: center;
	padding: 0;
	font-size: 32rpx;
	line-height: 44rpx;
}
.uni-numbox-disabled.yticon {
	color: #d6d6d6;
}

